        WA Serisi;;Makine;Pet Food;IMA Ciemme;Kutulama
        ";

        $str = explode("\n", $str);

        $brands = $this->core->QueryArray("SELECT id,`name{tr}` AS name FROM contents WHERE parent = 2334", [], PDO::FETCH_ASSOC, 'name');
        $product = $this->core->QueryArray("SELECT id,`name{tr}` AS name FROM contents WHERE parent = 2327", [], PDO::FETCH_ASSOC, 'name');
        $packageType = $this->core->QueryArray("SELECT id,`name{tr}` AS name FROM contents WHERE parent = 2333", [], PDO::FETCH_ASSOC, 'name');
        $products = [];

        for($i=0;$i<sizeof($str);$i++) {
          if(empty($str[$i][0]))
            continue;

          $str[$i] = explode(';',$str[$i]);

          if(!isset($packageType[$str[$i][5]])) {
            $sql = "INSERT INTO contents
              (`name{tr}`, `sl_module`, `parent`, `status`, `slug{tr}`)
                VALUES
              (
                '{$str[$i][5]}',
                '25',
                2333,
                1,
                '".$this->core->slug($str[$i][5])."'
              )
              ";

            $this->core->Query($sql);
            $packageType[$str[$i][5]] = ['id' => $this->core->lastInsertId()];
          }

          if(!isset($product[$str[$i][3]])) {
            $sql = "INSERT INTO contents
              (`name{tr}`, `sl_module`, `parent`, `status`, `slug{tr}`)
                VALUES
              (
                '{$str[$i][3]}',
                '25',
                2327,
                1,
                '".$this->core->slug($str[$i][3])."'
              )
              ";

            $this->core->Query($sql);
            $product[$str[$i][3]] = ['id' => $this->core->lastInsertId()];
          }

          $key = $this->core->slug(trim($str[$i][0]));

          $products[$key] = array(
            'name' => trim($str[$i][0]),
            'nodes' => [
              'code' => '',
              'brand' => $brands[$str[$i][4]]['id'],
              'packageType' => array_unique(array_merge(($products[$key]['nodes']['packageType'] ?? []), [$packageType[$str[$i][5]]['id']])),
              'product' => array_unique(array_merge(($products[$key]['nodes']['product'] ?? []), [$product[$str[$i][3]]['id']]))
            ]
          );
        }
        exit;

        /*
        1344	2429	mname{tr}		1
