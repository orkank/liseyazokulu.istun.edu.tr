        1419	2429	compare_codes{en}		1
        */

        $keys = array_keys($products);

        for($i=0;$i<sizeof($products);$i++) {
          $product = $products[$keys[$i]];

          $this->core->Query("INSERT INTO contents
              (`status`, `parent`, `sl_module`, `name{tr}`, `slug{tr}`)
            VALUES
              (1, 2422, 24, '{$product['name']}', '{$keys[$i]}')
          ");

          $id = $this->core->lastInsertId();

          $this->core->Query("INSERT INTO content_nodes
              (`cid`, `key`, `value`)
            VALUES
              ({$id}, 'brand', '{$product['nodes']['brand']}')
          ");

          array_map(function($e) use($id) {
            $this->core->Query("INSERT INTO content_nodes
                (`cid`, `key`, `value`)
              VALUES
                ({$id}, 'packageType', '{$e}')
            ");
          }, $product['nodes']['packageType']);

          array_map(function($e) use($id) {
            $this->core->Query("INSERT INTO content_nodes
                (`cid`, `key`, `value`)
              VALUES
                ({$id}, 'product', '{$e}')
            ");
          }, $product['nodes']['product']);
        }
      }
